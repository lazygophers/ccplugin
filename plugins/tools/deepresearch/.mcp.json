{
  "mcpServers": {
    "chrome-devtools": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "chrome-devtools-mcp@latest",
        "--acceptInsecureCerts",
        "--proxy-server=${PROXY_URL:-http://127.0.0.1:7890}"
      ],
      "env": {}
    },
    "duckduckgo": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "duckduckgo-mcp-server"
      ],
      "env": {
        "ALL_PROXY": "${PROXY_URL:-http://127.0.0.1:7890}",
        "HTTPS_PROXY": "${PROXY_URL:-http://127.0.0.1:7890}",
        "HTTP_PROXY": "${PROXY_URL:-http://127.0.0.1:7890}"
      }
    },
    "time": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "mcp-server-time",
        "--local-timezone=Asia/Shanghai"
      ],
      "env": {}
    },
    "sequential-thinking": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "mcp-server-sequential-thinking"
      ],
      "env": {}
    },
    "github": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "mcp-server-github"
      ],
      "env": {
        "GITHUB_TOKEN": "${GITHUB_TOKEN}",
        "ALL_PROXY": "${PROXY_URL:-http://127.0.0.1:7890}",
        "HTTPS_PROXY": "${PROXY_URL:-http://127.0.0.1:7890}",
        "HTTP_PROXY": "${PROXY_URL:-http://127.0.0.1:7890}"
      }
    },
    "gitlab": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "mcp-server-gitlab"
      ],
      "env": {
        "GITLAB_TOKEN": "${GITLAB_TOKEN}",
        "ALL_PROXY": "${PROXY_URL:-http://127.0.0.1:7890}",
        "HTTPS_PROXY": "${PROXY_URL:-http://127.0.0.1:7890}",
        "HTTP_PROXY": "${PROXY_URL:-http://127.0.0.1:7890}"
      }
    },
    "wikipedia": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "mcp-server-wikipedia"
      ],
      "env": {
        "ALL_PROXY": "${PROXY_URL:-http://127.0.0.1:7890}",
        "HTTPS_PROXY": "${PROXY_URL:-http://127.0.0.1:7890}",
        "HTTP_PROXY": "${PROXY_URL:-http://127.0.0.1:7890}"
      }
    }
  }
}
