{
  "name": "version",
  "version": "0.0.132",
  "description": "版本号管理插件 - 提供 SemVer 版本管理，支持自动版本更新和手动版本设置",
  "author": {
    "name": "lazygophers",
    "email": "admin@lazygophers.dev",
    "url": "https://github.com/lazygophers"
  },
  "homepage": "https://github.com/lazygophers/ccplugin/tree/master/plugins/version",
  "repository": "https://github.com/lazygophers/ccplugin/tree/master/plugins/version",
  "license": "AGPL-3.0-or-later",
  "keywords": [
    "version",
    "semver",
    "versioning",
    "release",
    "management"
  ],
  "hooks": {
    "SessionStart": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "PLUGIN_NAME=version uv run --directory ${CLAUDE_PLUGIN_ROOT} ./scripts/main.py hooks"
          }
        ]
      }
    ],
    "PreToolUse": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "PLUGIN_NAME=version uv run --directory ${CLAUDE_PLUGIN_ROOT} ./scripts/main.py hooks"
          }
        ]
      }
    ],
    "UserPromptSubmit": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "PLUGIN_NAME=version uv run --directory ${CLAUDE_PLUGIN_ROOT} ./scripts/main.py hooks"
          }
        ]
      }
    ]
  }
}
