  "author": {
    "name": "Your Name",
    "email": "your.email@example.com",
    "url": "https://github.com/yourusername"
  },
  "homepage": "https://github.com/yourusername/plugin-template",
  "repository": "https://github.com/yourusername/plugin-template",
