  "commands": [
    "./commands/command-template.md"
  ],
  "agents": [
    "./agents/agent-template.md"
  ],
  "skills": "./skills/",
  "mcpServers": "./.mcp.json",
  "outputStyles": "./styles/",
  "lspServers": "./.lsp.json",
