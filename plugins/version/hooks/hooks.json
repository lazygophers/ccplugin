{
  "description": "版本管理插件 Hooks - 自动管理项目版本号",
  "hooks": {
	"SessionStart": [
	  {
		"hooks": [
		  {
			"type": "command",
			"command": "uv run ${CLAUDE_PLUGIN_ROOT}/scripts/main.py --hooks",
		  }
		]
	  }
	],
	"UserPromptSubmit": [
	  {
		"hooks": [
		  {
			"type": "command",
			"command": "uv run ${CLAUDE_PLUGIN_ROOT}/scripts/main.py --hooks",
		  }
		]
	  }
	]
  }
}