{
  "name": "plugin-llms",
  "version": "0.0.135",
  "description": "llms.txt 标准插件 - 通过 Agent 自动生成符合 llms.txt 规范的文件",
  "author": {
    "name": "CCPlugin Contributors",
    "email": "info@lazygophers.com",
    "url": "https://github.com/lazygophers"
  },
  "homepage": "https://github.com/lazygophers/ccplugin",
  "repository": "https://github.com/lazygophers/ccplugin",
  "license": "AGPL-3.0-or-later",
  "keywords": [
    "llms",
    "llms.txt",
    "documentation",
    "llm",
    "context",
    "markdown",
    "agent"
  ],
  "agents": [
    "./agents/llms-generator.md"
  ],
  "skills": "./skills/",
  "hooks": {
    "SessionStart": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "PLUGIN_NAME=llms uv run --directory ${CLAUDE_PLUGIN_ROOT} ./scripts/main.py hooks"
          }
        ]
      }
    ]
  }
}
