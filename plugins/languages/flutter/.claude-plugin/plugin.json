{
  "name": "flutter",
  "version": "0.0.130",
  "description": "Flutter 开发插件 - 提供 Flutter 应用开发规范、设计系统应用、状态管理指导和代码智能支持",
  "author": {
    "name": "lazygophers",
    "email": "admin@lazygophers.dev",
    "url": "https://github.com/lazygophers"
  },
  "homepage": "https://github.com/lazygophers/ccplugin/tree/master/plugins/flutter",
  "repository": "https://github.com/lazygophers/ccplugin/tree/master/plugins/flutter",
  "license": "MIT",
  "keywords": [
    "flutter",
    "dart",
    "mobile-development",
    "ui-development",
    "state-management",
    "lsp"
  ],
  "agents": [
    "./agents/dev.md",
    "./agents/test.md",
    "./agents/debug.md",
    "./agents/perf.md"
  ],
  "skills": [
    "./skills/core/SKILL.md",
    "./skills/state/SKILL.md",
    "./skills/ui/SKILL.md",
    "./skills/android/SKILL.md",
    "./skills/ios/SKILL.md",
    "./skills/web/SKILL.md"
  ],
  "lspServers": "./.lsp.json",
  "hooks": {
    "SessionStart": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "PLUGIN_NAME=flutter uv run --directory ${CLAUDE_PLUGIN_ROOT} ./scripts/main.py hooks"
          }
        ]
      }
    ]
  }
}
