{
  "dart": {
    "command": "dart",
    "args": ["language-server", "--protocol=lsp"],
    "extensionToLanguage": {
      ".dart": "dart"
    },
    "settings": {
      "dart.sdkPath": "",
      "dart.flutterSdkPath": "",
      "dart.lineLength": 80,
      "dart.completionCommitCharacters": ["."],
      "dart.enableSdkFormatter": true,
      "dart.showTodos": true,
      "dart.reportAnalyzerErrors": "openErrors"
    },
    "initializationOptions": {
      "onlyAnalyzeProjectsWithOpenFiles": true,
      "suggestFromUnimportedLibraries": true,
      "closingLabels": true
    },
    "restartOnCrash": true,
    "maxRestarts": 5,
    "startupTimeout": 15000,
    "shutdownTimeout": 5000,
    "traceLogging": false
  }
}
