{
  "name": "csharp",
  "version": "0.0.129",
  "description": "C# 开发插件 - 提供 C# 12/.NET 8 开发规范、最佳实践和代码智能支持。包括异步编程、LINQ、ASP.NET Core 和 Entity Framework 指导",
  "author": {
    "name": "lazygophers",
    "email": "admin@lazygophers.dev",
    "url": "https://github.com/lazygophers"
  },
  "homepage": "https://github.com/lazygophers/ccplugin/tree/master/plugins/languages/csharp",
  "repository": "https://github.com/lazygophers/ccplugin/tree/master/plugins/languages/csharp",
  "license": "AGPL-3.0-or-later",
  "keywords": [
    "csharp",
    "c#",
    "dotnet",
    ".net",
    "development",
    "coding-style",
    "best-practices",
    "lsp",
    "omnisharp"
  ],
  "agents": [
    "./agents/dev.md",
    "./agents/test.md",
    "./agents/debug.md",
    "./agents/perf.md"
  ],
  "skills": [
    "./skills/core/SKILL.md",
    "./skills/async/SKILL.md",
    "./skills/linq/SKILL.md",
    "./skills/web/SKILL.md",
    "./skills/desktop/SKILL.md",
    "./skills/data/SKILL.md"
  ],
  "lspServers": "./.lsp.json",
  "hooks": {
    "SessionStart": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "PLUGIN_NAME=csharp uv run --directory ${CLAUDE_PLUGIN_ROOT} ./scripts/main.py hooks"
          }
        ]
      }
    ]
  }
}
