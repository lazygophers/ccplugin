{
  "name": "python",
  "version": "0.0.130",
  "description": "Python 开发插件 - 提供 Python 开发规范、最佳实践和代码智能支持",
  "author": {
    "name": "lazygophers",
    "email": "admin@lazygophers.dev",
    "url": "https://github.com/lazygophers"
  },
  "homepage": "https://github.com/lazygophers/ccplugin/tree/master/plugins/languages/python",
  "repository": "https://github.com/lazygophers/ccplugin/tree/master/plugins/languages/python",
  "license": "AGPL-3.0-or-later",
  "keywords": [
    "python",
    "py",
    "development",
    "coding-style",
    "best-practices",
    "pep8",
    "type-hints",
    "testing"
  ],
  "agents": [
    "./agents/dev.md",
    "./agents/test.md",
    "./agents/debug.md",
    "./agents/perf.md"
  ],
  "skills": [
    "./skills/core/SKILL.md",
    "./skills/error/SKILL.md",
    "./skills/types/SKILL.md",
    "./skills/testing/SKILL.md",
    "./skills/async/SKILL.md",
    "./skills/web/SKILL.md"
  ],
  "lspServers": "./.lsp.json",
  "hooks": {
    "SessionStart": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "PLUGIN_NAME=python uv run --directory ${CLAUDE_PLUGIN_ROOT} ./scripts/main.py hooks"
          }
        ]
      }
    ]
  }
}
