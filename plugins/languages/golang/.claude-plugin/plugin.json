{
  "name": "golang",
  "version": "0.0.134",
  "description": "Golang 开发插件 - 提供 Golang 开发规范、最佳实践和代码智能支持。包括通用 Golang 开发规范和基于 lazygophers 生态的最佳实践指导",
  "author": {
    "name": "lazygophers",
    "email": "admin@lazygophers.dev",
    "url": "https://github.com/lazygophers"
  },
  "homepage": "https://github.com/lazygophers/ccplugin/tree/master/plugins/golang",
  "repository": "https://github.com/lazygophers/ccplugin/tree/master/plugins/golang",
  "license": "AGPL-3.0-or-later",
  "keywords": [
    "golang",
    "go",
    "development",
    "coding-style",
    "best-practices",
    "lazygophers",
    "lsp",
    "gopls"
  ],
  "agents": [
    "./agents/dev.md",
    "./agents/test.md",
    "./agents/debug.md",
    "./agents/perf.md"
  ],
  "commands": [
    "./commands/dev.md",
    "./commands/review.md",
    "./commands/init.md",
    "./commands/ci.md"
  ],
  "skills": [
    "./skills/core/SKILL.md",
    "./skills/error/SKILL.md",
    "./skills/libs/SKILL.md",
    "./skills/naming/SKILL.md",
    "./skills/structure/SKILL.md",
    "./skills/testing/SKILL.md",
    "./skills/concurrency/SKILL.md",
    "./skills/lint/SKILL.md",
    "./skills/tooling/SKILL.md"
  ],
  "lspServers": "./.lsp.json",
  "hooks": {
    "SessionStart": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "PLUGIN_NAME=golang uv run --directory ${CLAUDE_PLUGIN_ROOT} ./scripts/main.py hooks"
          }
        ]
      }
    ],
    "PreToolUse": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "PLUGIN_NAME=golang uv run --directory ${CLAUDE_PLUGIN_ROOT} ./scripts/main.py hooks"
          }
        ]
      }
    ]
  }
}
