{
  "name": "naming",
  "version": "0.0.138",
  "description": "命名规范插件 - 提供跨编程语言的统一命名规范指南",
  "author": {
    "name": "lazygophers",
    "email": "admin@lazygophers.dev",
    "url": "https://github.com/lazygophers"
  },
  "homepage": "https://github.com/lazygophers/ccplugin/tree/master/plugins/languages/naming",
  "repository": "https://github.com/lazygophers/ccplugin/tree/master/plugins/languages/naming",
  "license": "AGPL-3.0-or-later",
  "keywords": [
    "naming",
    "conventions",
    "naming-conventions",
    "code-style",
    "standards",
    "cross-language"
  ],
  "skills": [
    "./skills/core/SKILL.md"
  ],
  "hooks": {
    "SessionStart": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "PLUGIN_NAME=naming uv run --directory ${CLAUDE_PLUGIN_ROOT} ./scripts/main.py hooks"
          }
        ]
      }
    ]
  }
}
