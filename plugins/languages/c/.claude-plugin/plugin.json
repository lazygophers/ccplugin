{
  "name": "c",
  "version": "0.0.138",
  "description": "C 开发插件 - 提供 C11/C17 开发规范、最佳实践和代码智能支持。包括系统编程、嵌入式开发、POSIX API 和内存管理指导",
  "author": {
    "name": "lazygophers",
    "email": "admin@lazygophers.dev",
    "url": "https://github.com/lazygophers"
  },
  "homepage": "https://github.com/lazygophers/ccplugin/tree/master/plugins/languages/c",
  "repository": "https://github.com/lazygophers/ccplugin/tree/master/plugins/languages/c",
  "license": "AGPL-3.0-or-later",
  "keywords": [
    "c",
    "c11",
    "c17",
    "development",
    "coding-style",
    "best-practices",
    "lsp",
    "clangd"
  ],
  "agents": [
    "./agents/dev.md",
    "./agents/test.md",
    "./agents/debug.md",
    "./agents/perf.md"
  ],
  "skills": [
    "./skills/core/SKILL.md",
    "./skills/memory/SKILL.md",
    "./skills/concurrency/SKILL.md",
    "./skills/posix/SKILL.md",
    "./skills/embedded/SKILL.md",
    "./skills/error/SKILL.md"
  ],
  "lspServers": "./.lsp.json",
  "hooks": {
    "SessionStart": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "PLUGIN_NAME=c uv run --directory ${CLAUDE_PLUGIN_ROOT} ./scripts/main.py hooks"
          }
        ]
      }
    ]
  }
}
