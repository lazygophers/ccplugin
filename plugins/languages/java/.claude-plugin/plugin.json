{
  "name": "java",
  "version": "0.0.131",
  "description": "Java 开发插件 - 提供 Java 21+ 开发规范、最佳实践和代码智能支持",
  "author": {
    "name": "lazygophers",
    "email": "admin@lazygophers.dev",
    "url": "https://github.com/lazygophers"
  },
  "homepage": "https://github.com/lazygophers/ccplugin/tree/master/plugins/languages/java",
  "repository": "https://github.com/lazygophers/ccplugin/tree/master/plugins/languages/java",
  "license": "AGPL-3.0-or-later",
  "keywords": [
    "java",
    "jdk",
    "development",
    "coding-style",
    "best-practices",
    "spring-boot",
    "lsp"
  ],
  "agents": [
    "./agents/dev.md",
    "./agents/test.md",
    "./agents/debug.md",
    "./agents/perf.md"
  ],
  "skills": [
    "./skills/core/SKILL.md",
    "./skills/error/SKILL.md",
    "./skills/concurrency/SKILL.md",
    "./skills/spring/SKILL.md",
    "./skills/performance/SKILL.md"
  ],
  "lspServers": "./.lsp.json",
  "hooks": {
    "SessionStart": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "PLUGIN_NAME=java uv run --directory ${CLAUDE_PLUGIN_ROOT} ./scripts/main.py hooks"
          }
        ]
      }
    ]
  }
}
