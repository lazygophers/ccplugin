{
  "name": "typescript",
  "version": "0.0.115",
  "description": "TypeScript 开发插件 - 提供 TypeScript 开发规范、最佳实践和代码智能支持。包括类型安全规范、严格模式最佳实践和基于最新 TS 5.9+ 的开发指导",
  "author": {
    "name": "lazygophers",
    "email": "admin@lazygophers.dev",
    "url": "https://github.com/lazygophers"
  },
  "homepage": "https://github.com/lazygophers/ccplugin/tree/master/plugins/typescript",
  "repository": "https://github.com/lazygophers/ccplugin/tree/master/plugins/typescript",
  "license": "MIT",
  "keywords": [
    "typescript",
    "ts",
    "development",
    "type-safety",
    "strict-mode",
    "vitest",
    "pnpm",
    "coding-style",
    "best-practices"
  ],
  "agents": [
    "./agents/dev.md",
    "./agents/test.md",
    "./agents/debug.md",
    "./agents/perf.md"
  ],
  "skills": "./skills/",
  "lspServers": "./.lsp.json",
  "hooks": {
    "SessionStart": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "PLUGIN_NAME=typescript uv run --directory ${CLAUDE_PLUGIN_ROOT} ./scripts/main.py hooks"
          }
        ]
      }
    ]
  }
}
